<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.3;net4</TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup>
    <Title>RFC 822</Title>
    <Description>Use this package if you need to parse or format RFC 822 formatted dates, like those found in RSS feeds. An RFC 822 formatted date time may look something like this: "Sun, 18 Nov 2012 20:32 +0100"</Description>
    <Authors>Michiel van Oosterhout</Authors>
    <PackageTags>system;data;async</PackageTags>
    <PackageProjectUrl>https://github.com/michielvoo/RFC-822</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.github.com/michielvoo/RFC-822/master/License.txt</PackageLicenseUrl>
    <PackageTags>RFC;RFC822;RSS</PackageTags>
    <AssemblyName>Rfc822</AssemblyName>
    <PackageId>Rfc822</PackageId>
    <IncludeSymbols>True</IncludeSymbols>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>1.0.4</Version>
  </PropertyGroup>

</Project>